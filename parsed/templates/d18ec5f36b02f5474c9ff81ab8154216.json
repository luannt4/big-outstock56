{
  "components/wbcomponents/addons/category": "<!-- BEGIN Side All Categories -->\r\n<div class=\"module-wrapper  sidebar--categories\" >\r\n    <h5 class=\"block-title\">{{lang 'category.label'}}</h5>\r\n    <div class=\"block-content clearfix\">\r\n        <ul class=\"navList\">\r\n            {{#each categories}}\r\n            <li class=\"navList-item\">\r\n                {{#if children}}\r\n                    <a class=\"navList-action has-subMenu{{#if id '==' ../../category.id}} is-current{{/if}}\" href=\"{{url}}\"> {{name}} </a>\r\n                    <a class=\"navList-toggle\" data-collapsible=\"navList-category-{{id}}\"><i class=\"icon navList-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\r\n\r\n                    <ul class=\"navList-subMenu-list\" id=\"navList-category-{{id}}\">\r\n                        {{#each children}}\r\n                            <li class=\"navList-subMenu-item\">\r\n                                {{#if children}}\r\n                                    <a class=\"navList-subMenu-action navList-action has-subMenu{{#if id '==' ../../../../category.id}} is-current{{/if}}\" href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n                                    <a class=\"navList-subMenu-toggle navList-toggle\" data-collapsible=\"navList-category-{{id}}\"><i class=\"icon navList-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\r\n                                    <ul class=\"navList-childList\" id=\"navList-category-{{id}}\">\r\n                                        {{#each children}}\r\n                                            <li class=\"navList-childList-item\">\r\n                                                <a class=\"navList-childList-action navList-action{{#if id '==' ../../../../../category.id}} is-current{{/if}}\" href=\"{{url}}\">{{name}}</a>\r\n                                            </li>\r\n                                        {{/each}}\r\n                                    </ul>\r\n                                {{else}}\r\n                                    <a class=\"navList-subMenu-action navList-action{{#if id '==' ../../../../category.id}} is-current{{/if}}\" href=\"{{url}}\">{{name}}</a>\r\n                                {{/if}}\r\n                            </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navList-action{{#if id '==' ../../category.id}} is-current{{/if}}\" href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n</div>\n"
