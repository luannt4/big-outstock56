  "components/wbcomponents/addons/wb_banner/home3/banner-item": "\r\n<div class=\"banners\" >\r\n\t\t\r\n\t{{#if icon '!=' ' '}}\r\n\t\t<a href=\"{{url}}\" class=\"banner-figure\"><i class=\"fa fa-{{icon}}\"></i></a>\r\n\t{{else}}\r\n\t<div class=\"banner-figure\">\r\n\t\t{{#if image '!=' ' '}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid \" src=\"{{cdn image }}\"  alt=\"{{title}}\"  /></a>\r\n\t\t{{else}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid\" src=\"{{cdn placeholder}}\" alt=\"{{title}}\"  /></a>\r\n\t\t{{/if}}\r\n\t</div>\r\n\t{{/if}}\r\n\t\r\n\t\r\n\t{{#if show_content}}\r\n\t<div class=\"banner-figcaption\">\r\n\t\t<div class=\"banner-figcaption__body\">\r\n\t\t\t{{#if title '!=' ' '}}\r\n\t\t\t<h5 class=\"banner-figcaption__title\"><a href=\"{{url}}\">{{title}}</a></h5>\r\n\t\t\t{{/if}}\r\n\r\n\t\t\t{{#if text '!=' ' '}}\r\n\t\t\t<div class=\"banner-figcaption__text\">{{{text}}}</div>\r\n\t\t\t{{/if}}\r\n\r\n\t\t\t{{#if button '!=' ' '}}\r\n\t\t\t<a href=\"{{url}}\" class=\"button button--primary  banner-figcaption__button\">{{button}}</a>\r\n\t\t\t{{/if}}\r\n\t\t</div>\r\n\t</div>\r\n\t{{/if}}\r\n</div>\r\n"
