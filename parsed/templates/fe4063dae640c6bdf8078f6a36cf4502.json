{
  "components/wbcomponents/addons/wb_megamobile/horizontal/submega": "<div class=\"row\">\n\t{{#if banner-mode '==' 'left'}}\n\t<div class=\"imageArea colRight col-lg-3 d-none d-lg-block\">\n\t\t{{#if banner-img '!=' ' '}}\n\t\t<a href=\"{{banner-url}}\"><img class=\"img-fluid\" src=\"{{cdn banner-img }}\"  alt=\"{{name}}\"  /></a>\n\t\t{{else}}\n\t\t<a href=\"{{banner-url}}\"><img class=\"img-fluid\" src=\"{{cdn banner-placeholder}}\" alt=\"{{name}}\"  /></a>\n\t\t{{/if}}\n\t</div>\n\t{{/if}}\n\t<div class=\"cateArea {{#if banner-mode '!=' ' '}}col-lg-9 col-md-12 columns-{{columns}} {{else}} col-md-12 {{/if}}\">\n\t\t<ul class=\"navPage-subMenu-list\">\n\t\t\t{{#each children}}\n\t\t\t\t<li class=\"navPage-subMenu-item\">\n\t\t\t\t\t{{#if children}}\n\t\t\t\t\t\t<a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n\t\t\t\t\t\t\t{{name}}\n\t\t\t\t\t\t</a>\n\t\t\t\t\t\t{{#if image}}\n\t\t\t\t\t\t<div class=\"banners navPage-subMenu-banner\">\n\t\t\t\t\t\t\t<a href=\"{{url}}\">\n\t\t\t\t\t\t\t\t<img class=\"lazyload\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{getImage image 'blog_size' }}\" alt=\"{{name}}\">\n\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t{{/if}}\n\t\t\t\t\t\t<ul class=\"navPage-childList\">\n\t\t\t\t\t\t\t{{#each children}}\n\t\t\t\t\t\t\t\t<li class=\"navPage-childList-item\">\n\t\t\t\t\t\t\t\t\t{{#if children}}\n\t\t\t\t\t\t\t\t\t\t<a class=\"navPage-childList-action navPages-action navPages-action-depth-max has-subMenu\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n\t\t\t\t\t\t\t\t\t\t\t{{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n\t\t\t\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t\t\t\t\t{{> components/common/navigation-dropdown}}\n\t\t\t\t\t\t\t\t\t{{else}}\n\t\t\t\t\t\t\t\t\t\t<a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n\t\t\t\t\t\t\t\t\t{{/if}}\n\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t{{/each}}\n\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t\n\n\t\t\t\t\t{{else}}\n\t\t\t\t\t\t<a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n\t\t\t\t\t{{/if}}\n\t\t\t\t</li>\n\t\t\t{{/each}}\n\t\t</ul>\n\t</div>\n\t\n\t{{#if banner-mode '==' 'right'}}\n\t<div class=\"imageArea colRight col-lg-3 d-none d-lg-block\">\n\t\t{{#if banner-img '!=' ' '}}\n\t\t<a href=\"{{banner-url}}\"><img class=\"lazyload\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn banner-img }}\"  alt=\"{{name}}\"  /></a>\n\t\t{{else}}\n\t\t<a href=\"{{banner-url}}\"><img class=\"lazyload\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn banner-placeholder}}\" alt=\"{{name}}\"  /></a>\n\t\t{{/if}}\n\t</div>\n\t{{/if}}\n</div>\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}