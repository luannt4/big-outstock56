{
  "components/wbcomponents/common/quick-search": "\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"wb-searchpro\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"input-group\">\r\n                <input class=\"form-control form-input\" data-search-quick name=\"search_query\"  data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <div class=\"input-group-append\"  >\r\n                    <button class=\"btn btn-link\" type=\"submit\">\r\n                        <span class=\"icon\">\r\n                            <svg class=\"lnr lnr-magnifier\"><use xlink:href=\"#lnr-magnifier\"></use></svg>\r\n                        </span>\r\n\r\n                    </button>\r\n                </div>\r\n            </div>\r\n            \r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults \" data-bind=\"html: results\"></section>\r\n \r\n"
