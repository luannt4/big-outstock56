{
  "components/common/breadcrumbs": "{{#unless theme_settings.hide_breadcrumbs }}\n<div class=\"wb-breadcrumbs \" itemscope itemtype=\"https://schema.org/BreadcrumbList\">\n        <div class=\"entry-header\">\n            <ul class=\"breadcrumb \">\n                {{#each breadcrumbs}}\n                <li class=\"breadcrumb-item {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\n                    <a class=\"breadcrumb-label\"\n                       itemprop=\"item\"\n                       href=\"{{url}}\"\n                       {{#if @last}}aria-current=\"page\"{{/if}}\n                    >\n                    <span itemprop=\"name\">{{name}}</span>\n                    </a>\n                    <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\n                    {{#unless @last}}<svg width=\"16\" height=\"16\" class=\"icon\"><use xlink:href=\"#icon-chevron-right\"></use></svg> {{/unless}}\n\n                </li>\n\n                {{/each}}\n            </ul>\n        </div>\n\n</div>\n{{/unless}}\n"
}